{
	"name": "coblocks/event-item",
	"category": "coblocks",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__title",
			"default": "Rich's Birthday Party"
		},
		"description": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__description",
			"default": "Vivamus sagittis lacus vel augue laoreet rutrum faucibus dolor auctor."
		},
		"eventDay": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__day",
			"default": "21"
		},
		"eventMonth": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__month",
			"default": "September"
		},
		"eventYear": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__year",
			"default": "2019"
		},
		"eventTime": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__time",
			"default": "7:00pm"
		},
		"eventLocation": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-events__location",
			"default": "NYC"
		},
		"textColor": {
			"type": "string"
		},
		"customTextColor": {
			"type": "string"
		},
		"pageNum": {
			"type": "number",
			"default": 0
		},
		"lastItem": {
			"type": "boolean",
			"default": false
		}
	}
}
